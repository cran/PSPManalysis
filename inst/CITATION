citHeader("To cite PSPManalysis in publications use:")

citEntry(entry = "Manual",
  title        = "PSPManalysis: A Software Package for Numerical Analysis of Physiologically Structured Population Models",
  author       = "André M. de Roos",
  organization = "Institute for Biodiversity and Ecosystem Dynamics, University of Amsterdam",
  year         = "2017",
  url          = "https://bitbucket.org/amderoos/PSPManalysis",

  textVersion  =
  paste("André M. de Roos (2017).",
        "PSPManalysis: A Software Package for Numerical Analysis of Physiologically Structured Population Models",
        "URL https://bitbucket.org/amderoos/PSPManalysis")
)
